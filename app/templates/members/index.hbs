<h3>All Team Member</h3>
    <ol>
        {{#each model as |user|}}
          <li>
              {{#link-to 'members.member' user}} 
                    {{user.name}} and his/her favorite food is {{user.favoriteFood}}
              {{/link-to}}
          </li>  
        {{/each}}
    </ol>
   
{{outlet}}